pipeline{
    agent any

    stages{
        stage('Checkout'){
            steps{
                git url: 'https://github.com/Axors3/Proyecto-DevOps.git',branch: 'dev'
            }
        }
        stage('Build'){
            steps{
                sh 'npm install'
            }
        }
        stage('Testing'){
            steps{
                sh 'npm run test'
            }
        }
        stage('Deploy Pipeline 2'){
            steps{
                build 'uadyfon-api-backend-deploy'
            }
        }

    }
}